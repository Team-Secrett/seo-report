@article{what_is_seo,
title = {What is search engine optimization: SEO?},
journal = {Procedia - Social and Behavioral Sciences},
volume = {9},
pages = {487-493},
year = {2010},
note = {World Conference on Learning, Teaching and Administration Papers},
issn = {1877-0428},
doi = {https://doi.org/10.1016/j.sbspro.2010.12.185},
url = {https://www.sciencedirect.com/science/article/pii/S1877042810022901},
author = {Nursel Yalçın and Utku Köse},
keywords = {SEO, Search engine optimization, Page rank, Backlink, Bot, Meta tags, Social media},
abstract = {Web sites, which index and class other web sites according to their keywords, explanations and contents and make it easier and faster to reach obtained site-search results, are called as search engines. SEO “Search Engine Optimization” is the one of the widely used technique that provides web sites fast reachable. In this work we explained required information to make a web site more indexed by search engines and considering their keywords make them first listed.}
}

